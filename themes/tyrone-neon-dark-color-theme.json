{
  "name": "Tyrone Neon",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    // === FOCUS BORDER ===
    "focusBorder": "#000000",

    // === EDITOR COLORS ===
    "editor.background": "#000000",
    "editor.foreground": "#E5E5E5",
    "editorLineNumber.foreground": "#484F58",
    "editorLineNumber.activeForeground": "#DAFF01",
    "editor.selectionBackground": "#171717",
    "editor.selectionHighlightBackground": "#171717",
    "editor.inactiveSelectionBackground": "#171717",
    "editor.wordHighlightBackground": "#FF6B9D30",
    "editor.wordHighlightStrongBackground": "#FF6B9D50",
    "editor.findMatchBackground": "#00D9FF60",
    "editor.findMatchHighlightBackground": "#00D9FF30",
    "editor.hoverHighlightBackground": "#3E3E3E",
    "editor.lineHighlightBackground": "#000000",
    "editorCursor.foreground": "#DAFF01",
    "editorWhitespace.foreground": "#484F58",
    "editorIndentGuide.background1": "#000000",
    "editorIndentGuide.activeBackground1": "#DAFF01",
    "editorIndentGuide.activeBackground2": "#DAFF01",
    "editorIndentGuide.activeBackground3": "#DAFF01",
    "editorIndentGuide.activeBackground4": "#DAFF01",
    "editorIndentGuide.activeBackground5": "#DAFF01",
    "editorIndentGuide.activeBackground6": "#DAFF01",
    "editorRuler.foreground": "#000000",
    "editorBracketMatch.background": "#171717",
    "editorBracketMatch.border": "#DAFF01",

    // === EDITOR GUTTER ===
    "editorGutter.background": "#000000",
    "editorGutter.modifiedBackground": "#00D9FF",
    "editorGutter.addedBackground": "#00FF87",
    "editorGutter.deletedBackground": "#FF4757",

    // === EDITOR GROUPS & TABS ===
    "editorGroup.border": "#000000",
    "editorGroupHeader.tabsBackground": "#000000",
    "tab.activeBackground": "#000000",
    "tab.inactiveBackground": "#000000",
    "tab.activeForeground": "#E5E5E5",
    "tab.inactiveForeground": "#7D8590",
    "tab.border": "#000000",
    "tab.activeBorder": "#DAFF01",
    "tab.unfocusedActiveBorder": "#484F58",
    "tab.hoverBackground": "#000000",

    // === SIDEBAR ===
    "sideBar.background": "#000000",
    "sideBar.foreground": "#E5E5E5",
    "sideBar.border": "#000000",
    "sideBarTitle.foreground": "#DAFF01",
    "sideBarSectionHeader.background": "#000000",
    "sideBarSectionHeader.foreground": "#E5E5E5",
    "sideBarSectionHeader.border": "#000000",

    // === ACTIVITY BAR ===
    "activityBar.background": "#000000",
    "activityBar.foreground": "#E5E5E5",
    "activityBar.border": "#000000",
    "activityBar.activeBorder": "#DAFF01",
    "activityBarBadge.background": "#DAFF01",
    "activityBarBadge.foreground": "#000000",

    // === STATUS BAR ===
    "statusBar.background": "#000000",
    "statusBar.foreground": "#E5E5E5",
    "statusBar.border": "#000000",
    "statusBar.debuggingBackground": "#FF4757",
    "statusBar.debuggingForeground": "#FFFFFF",
    "statusBar.noFolderBackground": "#000000",
    "statusBarItem.hoverBackground": "#000000",
    "statusBarItem.activeBackground": "#171717",

    // === TITLE BAR ===
    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#E5E5E5",
    "titleBar.inactiveBackground": "#000000",
    "titleBar.inactiveForeground": "#7D8590",
    "titleBar.border": "#000000",

    // === MENU BAR ===
    "menubar.selectionForeground": "#E5E5E5",
    "menubar.selectionBackground": "#171717",
    "menu.foreground": "#E5E5E5",
    "menu.background": "#000000",
    "menu.selectionForeground": "#E5E5E5",
    "menu.selectionBackground": "#171717",

    // === COMMAND PALETTE ===
    "quickInput.background": "#000000",
    "quickInput.foreground": "#E5E5E5",
    "quickInputTitle.background": "#000000",

    // === LISTS ===
    "list.activeSelectionBackground": "#171717",
    "list.activeSelectionForeground": "#E5E5E5",
    "list.inactiveSelectionBackground": "#171717",
    "list.inactiveSelectionForeground": "#E5E5E5",
    "list.hoverBackground": "#000000",
    "list.hoverForeground": "#E5E5E5",
    "list.focusBackground": "#171717",
    "list.focusForeground": "#E5E5E5",

    // === BUTTONS ===
    "button.background": "#DAFF01",
    "button.foreground": "#000000",
    "button.hoverBackground": "#b8d400",
    "button.secondaryBackground": "#000000",
    "button.secondaryForeground": "#E5E5E5",
    "button.secondaryHoverBackground": "#000000",

    // === INPUT FIELDS ===
    "input.background": "#000000",
    "input.foreground": "#E5E5E5",
    "input.border": "#000000",
    "input.placeholderForeground": "#7D8590",
    "inputOption.activeBackground": "#171717",
    "inputValidation.errorBackground": "#FF4757",
    "inputValidation.errorBorder": "#FF4757",

    // === SCROLLBAR ===
    "scrollbar.shadow": "#000000",
    "scrollbarSlider.background": "#484F5880",
    "scrollbarSlider.hoverBackground": "#484F58",
    "scrollbarSlider.activeBackground": "#DAFF01",

    // === BADGE ===
    "badge.background": "#DAFF01",
    "badge.foreground": "#000000",

    // === PROGRESS BAR ===
    "progressBar.background": "#DAFF01",

    // === PANEL (Terminal, Output, etc.) ===
    "panel.background": "#000000",
    "panel.border": "#000000",
    "panelTitle.activeBorder": "#DAFF01",
    "panelTitle.activeForeground": "#E5E5E5",
    "panelTitle.inactiveForeground": "#7D8590",

    // === TERMINAL ===
    "terminal.background": "#000000",
    "terminal.foreground": "#E5E5E5",
    "terminal.ansiBlack": "#484F58",
    "terminal.ansiRed": "#FF4757",
    "terminal.ansiGreen": "#00FF87",
    "terminal.ansiYellow": "#FFFA65",
    "terminal.ansiBlue": "#00D9FF",
    "terminal.ansiMagenta": "#FF6B9D",
    "terminal.ansiCyan": "#00FFCC",
    "terminal.ansiWhite": "#E5E5E5",
    "terminal.ansiBrightBlack": "#7D8590",
    "terminal.ansiBrightRed": "#FF6B6B",
    "terminal.ansiBrightGreen": "#5EFFA3",
    "terminal.ansiBrightYellow": "#FFFF80",
    "terminal.ansiBrightBlue": "#40E0FF",
    "terminal.ansiBrightMagenta": "#FF8FC7",
    "terminal.ansiBrightCyan": "#80FFE6",
    "terminal.ansiBrightWhite": "#FFFFFF",

    // === NOTIFICATIONS ===
    "notifications.background": "#000000",
    "notifications.foreground": "#E5E5E5",
    "notifications.border": "#000000",
    "notificationCenter.border": "#000000",
    "notificationCenterHeader.background": "#000000",

    // === EXTENSIONS ===
    "extensionButton.prominentBackground": "#DAFF01",
    "extensionButton.prominentForeground": "#000000",
    "extensionButton.prominentHoverBackground": "#b8d400",

    // === PEEK VIEW ===
    "peekView.border": "#DAFF01",
    "peekViewEditor.background": "#000000",
    "peekViewResult.background": "#000000",
    "peekViewTitle.background": "#000000",

    // === MERGE CONFLICTS ===
    "merge.currentHeaderBackground": "#00FF8740",
    "merge.currentContentBackground": "#00FF8720",
    "merge.incomingHeaderBackground": "#00D9FF40",
    "merge.incomingContentBackground": "#00D9FF20",

    // === GIT DECORATIONS ===
    "gitDecoration.modifiedResourceForeground": "#00D9FF",
    "gitDecoration.deletedResourceForeground": "#FF4757",
    "gitDecoration.untrackedResourceForeground": "#00FF87",
    "gitDecoration.ignoredResourceForeground": "#7D8590",
    "gitDecoration.conflictingResourceForeground": "#FFFA65",

    // === BREADCRUMBS ===
    "breadcrumb.foreground": "#7D8590",
    "breadcrumb.background": "#000000",
    "breadcrumb.focusForeground": "#E5E5E5",
    "breadcrumb.activeSelectionForeground": "#DAFF01",
    "breadcrumbPicker.background": "#000000"
  },
  "tokenColors": [
    // === COMMENTS ===
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7D8590",
        "fontStyle": "italic"
      }
    },

    // === KEYWORDS ===
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#FF4757",
        "fontStyle": "bold"
      }
    },

    // === STRINGS ===
    {
      "name": "String",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#00FF87"
      }
    },

    // === NUMBERS ===
    {
      "name": "Number",
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#FFFA65"
      }
    },

    // === FUNCTIONS ===
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#00D9FF"
      }
    },

    // === CLASSES & TYPES ===
    {
      "name": "Class",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#FF6B9D",
        "fontStyle": "bold"
      }
    },

    // === VARIABLES ===
    {
      "name": "Variable",
      "scope": ["variable", "meta.definition.variable"],
      "settings": {
        "foreground": "#E5E5E5"
      }
    },

    // === PARAMETERS ===
    {
      "name": "Parameter",
      "scope": ["variable.parameter", "meta.parameters"],
      "settings": {
        "foreground": "#C7A3FF",
        "fontStyle": "italic"
      }
    },

    // === PROPERTIES ===
    {
      "name": "Property",
      "scope": ["variable.object.property", "meta.object-literal.key"],
      "settings": {
        "foreground": "#FFB347"
      }
    },

    // === OPERATORS ===
    {
      "name": "Operator",
      "scope": ["keyword.operator", "punctuation"],
      "settings": {
        "foreground": "#00FF87"
      }
    },

    // === HTML TAGS ===
    {
      "name": "HTML Tag",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#FF4757"
      }
    },

    // === HTML ATTRIBUTES ===
    {
      "name": "HTML Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#00D9FF"
      }
    },

    // === CSS SELECTORS ===
    {
      "name": "CSS Selector",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#FF6B9D"
      }
    },

    // === CSS PROPERTIES ===
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#00FFCC"
      }
    },

    // === JSON KEYS ===
    {
      "name": "JSON Key - Level 0",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#FF6B9D"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "support.type.property-name.json meta.structure.dictionary.json"
      ],
      "settings": {
        "foreground": "#00D9FF"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "support.type.property-name.json meta.structure.dictionary.json meta.structure.dictionary.json"
      ],
      "settings": {
        "foreground": "#00FFCC"
      }
    },

    // === MARKDOWN ===
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading", "entity.name.section.markdown"],
      "settings": {
        "foreground": "#DAFF01",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#E5E5E5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#E5E5E5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#00FF87"
      }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#00D9FF"
      }
    },

    // === PYTHON ===
    {
      "name": "Python Self",
      "scope": ["variable.language.special.self.python"],
      "settings": {
        "foreground": "#FF4757",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python Decorator",
      "scope": ["entity.name.function.decorator.python"],
      "settings": {
        "foreground": "#FFFA65"
      }
    },

    // === JAVASCRIPT/TYPESCRIPT ===
    {
      "name": "JS/TS This",
      "scope": ["variable.language.this"],
      "settings": {
        "foreground": "#FF4757",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JS/TS Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#FF4757"
      }
    },
    {
      "name": "TypeScript Types",
      "scope": [
        "entity.name.type.ts",
        "entity.name.type.tsx",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#FF6B9D"
      }
    }
  ],
  "semanticTokenColors": {
    "variable": "#E5E5E5",
    "variable.readonly": "#C7A3FF",
    "parameter": "#C7A3FF",
    "function": "#00D9FF",
    "method": "#00D9FF",
    "class": "#FF6B9D",
    "interface": "#FF6B9D",
    "type": "#FF6B9D",
    "enum": "#FF6B9D",
    "keyword": "#FF4757",
    "string": "#00FF87",
    "number": "#FFFA65",
    "comment": "#7D8590",
    "property": "#FFB347",
    "namespace": "#DAFF01"
  }
}
